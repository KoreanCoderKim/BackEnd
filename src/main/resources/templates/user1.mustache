{{>header}}
<form class = "container" action = "/create" method = "post" accept-charset="utf-8">
    <div class = "mb-3">
        <label class = "form-label">Name</label>
        <textarea class = "form-control" rows = "3" name = "Name"></textarea>
    </div>
    <div class = "mb-3">
        <label class = "form-label">contents</label>
        <textarea class = "form-control" rows = "3" name = "content"></textarea>
    </div>
    <button type = "submit" class = "btn btn-primary">Submit(Update one more click Please...)</button>
</form>
<table>
    <tbody>
    {{#articleList}}
        <tr>
            <td>-{{Name}}: </td>
            <td>{{content}}</td>
        </tr>
    {{/articleList}}
    </tbody>
</table>
{{>footer}}